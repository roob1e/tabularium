{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Tabularium",
  "version": "0.1.0",
  "identifier": "com.tabularium.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "resizable": true,
        "maximized": true,
        "title": ""
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png",
      "icons/icon.png",
      "icons/icon.png",
      "icons/icon.png",
      "icons/icon.png"
    ]
  }
}
